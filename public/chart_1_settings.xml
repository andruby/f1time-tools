<settings>
  <text_size>10</text_size>
  <hide_bullets_count>18</hide_bullets_count>
  <data_type>csv</data_type>
  <background>
    <alpha>90</alpha>
    <border_color>000000</border_color>
    <border_alpha>100</border_alpha>
  </background>
  <plot_area>
    <margins>
      <left>40</left>
      <right>50</right>
      <top>40</top>
      <bottom>65</bottom>
    </margins>
  </plot_area>
  <grid>
    <x>
      <alpha>10</alpha>
      <dash_length>1</dash_length>
      <approx_count>13</approx_count>
    </x>
    <y_left>
      <alpha>10</alpha>
    </y_left>
    <y_right>
      <enabled>0</enabled>
    </y_right>
  </grid>
  <axes>
    <x>
      <width>0</width>
      <color>0D8ECF</color>
    </x>
    <y_left>
      <width>0</width>
      <color>0D8ECF</color>
    </y_left>
    <y_right>
      <width>0</width>
      <color>0D8ECF</color>
      <alpha>97</alpha>
    </y_right>
  </axes>
  <values>
    <y_left>
      <skip_first>0</skip_first>
      <unit>s</unit>
    </y_left>
    <y_right>
      <min>0</min>
      <skip_first>0</skip_first>
      <unit>s</unit>
    </y_right>
  </values>
  <indicator>
    <color>0D8ECF</color>
    <x_balloon_text_color>FFFFFF</x_balloon_text_color>
    <line_alpha>50</line_alpha>
    <selection_color>0D8ECF</selection_color>
    <selection_alpha>20</selection_alpha>
  </indicator>
  <zoom_out_button>
    <text_color_hover>FF0F00</text_color_hover>
  </zoom_out_button>
  <help>
    <button>
      <color>FCD202</color>
      <text_color>000000</text_color>
      <text_color_hover>FF0F00</text_color_hover>
    </button>
    <balloon>
      <text><![CDATA[Click on the graph to turn on/off value baloon <br/><br/>Click on legend key to show/hide graph<br/><br/>Mark the area you wish to enlarge]]></text>
      <color>FCD202</color>
      <text_color>000000</text_color>
    </balloon>
  </help>
  <graphs>
    <graph gid="0">
      <title>Laptime</title>
      <color>3366FF</color>
      <color_hover>3366FF</color_hover>
      <fill_alpha>4</fill_alpha>
      <balloon_alpha>80</balloon_alpha>
      <balloon_text><![CDATA[<b>{value} s</b>]]></balloon_text>
    </graph>
    <graph gid="1">
      <title>Gap</title>
      <axis>right</axis>
      <color>FFCC33</color>
      <color_hover>FFCC33</color_hover>
      <selected>0</selected>
      <balloon_alpha>80</balloon_alpha>
      <balloon_text_color>000000</balloon_text_color>
      <balloon_text><![CDATA[<b>{value} s</b>]]></balloon_text>
    </graph>
  </graphs>
  <labels>
    <label lid="0">
      <text>Laptimes and Gap</text>
      <y>12</y>
      <text_size>13</text_size>
      <align>center</align>
    </label>
  </labels>
</settings>